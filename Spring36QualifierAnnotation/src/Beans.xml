<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
	xmlns:context="http://www.springframework.org/schema/context">

	<context:annotation-config />

	<bean id="EmployeeBC" class="com.mistertandon.model.Employee">
		<constructor-arg name="id" value="104981"></constructor-arg>
		<constructor-arg name="name" value="Mister Tandon"></constructor-arg>
	</bean>

	<bean id="PanCardA" class="com.mistertandon.model.PanCard">
		<property name="panNumber" value="ZQRQS4803A"></property>
		<property name="panHolderName" value="Mister Tandon"></property>
	</bean>

	<bean id="PanCardB" class="com.mistertandon.model.PanCard">
		<property name="panNumber" value="ZQRQS4803B"></property>
		<property name="panHolderName" value="Praveen Tandon"></property>
	</bean>
</beans>